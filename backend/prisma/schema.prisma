generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Wallet {
  @@index([walletSetId])
  id           String   @id @default(uuid())
  state        String
  walletSetId  String
  custodyType  String
  address      String   @unique
  blockchain   String
  accountType  String
  updateDate   DateTime
  createDate   DateTime
}
